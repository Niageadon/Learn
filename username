[32m+[m[32m*/[m
[32m+[m
[32m+[m[32mfunction randomInteger(minValue, maxValue){[m
[32m+[m[32m  let answer = {value: 0, index: 0};[m
[32m+[m[32m  let value;[m
[32m+[m[32m  for (let i = minValue; i <= maxValue; i++){[m
[32m+[m[32m    value = Math.random();[m
[32m+[m[32m    if (answer.value < value) {[m
[32m+[m[32m      answer.value = value;[m
[32m+[m[32m      answer.index = i;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  return answer.index[m
[32m+[m[32m  //return  Math.round(minValue + Math.random() * (maxValue - minValue))[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mvar time = performance.now();[m
[32m+[m[32mfor(let i = 0; i<1000; i++){[m
[32m+[m[32m  randomInteger(1, 3333);[m
[32m+[m[32m}[m
[32m+[m[32mtime = performance.now() - time;[m
[32m+[m[32mconsole.log(time)[m
